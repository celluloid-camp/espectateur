{
  "extends": "@celluloid/config/tsconfig/internal-package.json",
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "noEmitOnError": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noImplicitAny": false,
    "strict": false,
    "checkJs": false,
    "allowJs": true
  }
}
